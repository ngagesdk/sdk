//----------------------------------------------------
//
//    r_popfield_three_text_field_dialog
//   
//
//----------------------------------------------------
//
RESOURCE DIALOG r_popfield_three_text_field_dialog
    {
    flags = EEikDialogFlagNoDrag | EEikDialogFlagFillAppClientRect |
            EEikDialogFlagNoTitleBar | EEikDialogFlagCbaButtons | 
            EEikDialogFlagNoBorder | EEikDialogFlagWait;
    buttons = R_AVKON_SOFTKEYS_OK_CANCEL;
    form = r_popfield_three_text_field_form;
    }


//----------------------------------------------------
//
//    r_popfield_three_text_field_form
//   
//
//----------------------------------------------------
//
RESOURCE FORM r_popfield_three_text_field_form
    {
    items =
        {
        DLG_LINE
           {
           type = EAknCtPopupFieldText;
           prompt = qtn_aknexgrid_filling_order_primary;
           id = EAknExGridDialogFillingOrderPrimaryOrientation;
           itemflags = EEikDlgItemTakesEnterKey | EEikDlgItemOfferAllHotKeys;
           control = POPUP_FIELD_TEXT
                {
                popupfield = POPUP_FIELD
                    {
                    width = AKNEXPOPFIELD_TEXT_WIDTH;
                    };
                textarray = r_popfield_two_VertHoriztext_array;
                };
           tooltip = qtn_aknexgrid_filling_order_popfield;
           },
        DLG_LINE
           {
           type = EAknCtPopupFieldText;
           prompt = qtn_aknexgrid_filling_order_vert_direction;
           id = EAknExGridDialogFillingOrderVerticalDirection;
           itemflags = EEikDlgItemTakesEnterKey | EEikDlgItemOfferAllHotKeys;
           control = POPUP_FIELD_TEXT
                {
                popupfield = POPUP_FIELD
                    {
                    width = AKNEXPOPFIELD_TEXT_WIDTH;
                    };
                textarray = r_popfield_two_BotToptext_array;
                };
           tooltip = qtn_aknexgrid_filling_order_popfield;
           },
        DLG_LINE
           {
           type = EAknCtPopupFieldText;
           prompt = qtn_aknexgrid_filling_order_horiz_direction;
           id = EAknExGridDialogFillingOrderHorizontalDirection;
           itemflags = EEikDlgItemTakesEnterKey | EEikDlgItemOfferAllHotKeys;
           control = POPUP_FIELD_TEXT
                {
                popupfield = POPUP_FIELD
                    {
                    width = AKNEXPOPFIELD_TEXT_WIDTH;
                    };
                textarray = r_popfield_two_RightLefttext_array;
                };
           tooltip = qtn_aknexgrid_filling_order_popfield;
           }
        };
    }

