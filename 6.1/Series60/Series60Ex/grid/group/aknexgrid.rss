//----------------------------------------------------
//   
//    r_aknexgrid_navi_decorator
//    Status pane definition of details
//
//----------------------------------------------------
//
RESOURCE NAVI_DECORATOR r_aknexgrid_navi_decorator
    {
    type = ENaviDecoratorControlTabGroup;
    control = TAB_GROUP
        {
        tab_width = EAknTabWidthWithTwoLongTabs;  // two tabs
        active = 0;
        tabs = 
            {
            TAB
                {
                id = EAknExGridViewTab; // from application hrh
                txt = qtn_aknexnotif_view_main_tab;
                },
            TAB
                {
                id = EAknExGridViewSelectionTab;
                txt = qtn_aknexnotif_view_selection_tab;
                },
            TAB
                {
                id = EAknExGridViewMenuTab;
                txt = qtn_aknexnotif_view_menu_tab;
                },
            TAB
                {
                id = EAknExGridViewMarkableSelectionTab;
                txt = qtn_aknexnotif_view_markable_selection_tab;
                },
            TAB
                {
                id = EAknExGridViewGMSTab;
                txt = qtn_aknexnotif_view_gms_tab;
                }
            };
        };
    }


