<!-- Parameter elements -->

<!-- The CS-Help project file -->
<!ELEMENT cshproj (helpfileUID,directories,files)>

<!-- directories -->
<!ELEMENT helpfileUID (#PCDATA)>

<!-- directories -->
<!ELEMENT directories (input,output,headers,graphics,working)>

<!-- input -->
<!ELEMENT input (#PCDATA)>

<!-- output -->
<!ELEMENT output (#PCDATA)>

<!-- headers -->
<!ELEMENT headers (#PCDATA)>

<!-- graphics -->
<!ELEMENT graphics (#PCDATA)>

<!-- working -->
<!ELEMENT working (#PCDATA)>

<!-- files -->
<!ELEMENT files (source,graphicssource,destination,customization)>

<!-- source -->
<!ELEMENT source (file|xmlfile)+>

<!-- graphics -->
<!ELEMENT graphicssource (file)+>

<!-- files -->
<!ELEMENT file (#PCDATA)>
<!ELEMENT xmlfile (#PCDATA)>

<!-- destination -->
<!ELEMENT destination (#PCDATA)>

<!-- customization -->
<!ELEMENT customization (#PCDATA)>





